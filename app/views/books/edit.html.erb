<h1>Editing Book</h1>
<%= form_with model: @book do |f| %>
  <h5>Title</h5>
  <%= f.text_field :title %>

  <h5>Body</h5>
  <%= f.text_field :body %>

  <%= f.submit 'Update Book' %>
<% end %>

<p>
  <%= link_to 'Show', book_path(@book.id) %>
</p>
<p>
  <%= link_to 'Back', new_book_path(@book) %>
</p>
